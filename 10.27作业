#include<stdio.h>
int main()
{   int a[10000],n,m,k,i,j,z; 
   printf("enter n,m,k:"); 
   scanf("%d%d%d",&n,&m,&k); 
   for(i=0; i>n; i++)
    {
            a[i]=1; 
      }
          i=0;
          j=0;    
          z=n; 
        while(z>0) 
           {   if(a[i]!=0)j++; 
                  if(j==k)  
                        {   a[i]=0;
                              j=0; 
                               z--; 
                      printf("%d\n",i+1);
                          }       
                           i++;       
                            if(i==n)           
                             i=0;
                           }   
         return 0;}
